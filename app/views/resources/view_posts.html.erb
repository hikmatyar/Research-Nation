<%= javascript_include_tag "jquery.autocomplete"%>
<% content_for :html_title, 'Browse Publications' %>

<%= stylesheet_link_tag 'autocomplete'%>
<%= javascript_include_tag 'posts_search' %>

<div class="content">
  <div class="profile_tabs">
    <ul class="tabs">
      <li style="-moz-border-radius-topright: 10px !important; -webkit-border-top-right-radius: 10px !important;">
        <%= link_to 'Profiles', { :controller => 'profiles', :action => 'view_profile_list'}, { :class => 'profiles' }%>
      </li>

      <li style="-moz-border-radius-topleft: 10px !important; -webkit-border-top-left-radius: 10px !important;">
        <%= link_to 'Publications', { :controller => 'resources', :action => 'view_posts' }, { :class => 'posts' } %>
      </li>
    </ul>
  </div>
  <div class="search_contant">

    <div class="left_content" id="left_content">

      <%= render :partial => 'posts'%>
      <div id="resource_pagination">
        <%= will_paginate @resources %>
      </div>
    </div>
    <div style="height:300px;">
    <div class="search_right">
      <%= render :partial => 'right_content' %>
    </div>

  </div>
</div>

<div>
</div>
