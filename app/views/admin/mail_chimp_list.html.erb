<div class="tab_container">
<table border="0" cellspacing="5px" cellpadding="5px" id="mail_chimp_list_table">
  <tbody>
    <tr>

      <th>First Name</th>
      <th>Last Name</th>
      <th>Email Address</th>
      <th>Subscribed At</th>
    </tr>
    <% @members.each do |m|%>
      <tr>

        <td><%= m.FNAME %></td>
        <td><%= m.LNAME %></td>
        <td><%= m.email %></td>
        <td ><%= m.timestamp.to_date.strftime("%B %d, %Y") %></td>
      </tr>
    <% end %>
  <tbody>
</table>
</div>
