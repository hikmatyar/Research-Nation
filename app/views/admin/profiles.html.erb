<div class="tab_container">
<table border="0" cellspacing="20px" cellpadding="20px" id="users_table">
  <tbody>
    <tr>
      <th>No.</th>
      <th>Name</th>
      <th>Country</th>
      <th>Website</th>
      <th></th>
    </tr>
    <% @profiles.each_with_index do |p, index|%>
      <tr>
        <td><%= @profiles_count - index %></td>
        <td><%= (link_to p.name, :controller => 'profiles', :action => 'edit_company_profile', :id => p.id) if p.user.company_seller? %>
        <%= (link_to p.name, :controller => 'profiles', :action => 'edit_individual_profile', :id => p.id) unless p.user.company_seller? %>
        </td>
        <td><%=  %></td>
        <td ><%= p.country %></td>
        <td ><%= p.website %></td>
        <td ><%= link_to 'Delete', { :controller => 'profiles', :action => 'delete', :id => p.id }, :confirm => "Are you sure you want to delete this post?"  %></td>
      </tr>
    <% end %>
  <tbody>
</table>
</div>
